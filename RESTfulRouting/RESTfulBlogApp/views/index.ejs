<% include ./partials/header %>

<h1>index.ejs Template Page</h1>

<% blogs.forEach(function(blog){ %>
<div>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>">
    <span> <%= blog.created.toDateString() %> </span>
    <p> <%= blog.body %> </p>
    <a href="/blogs/<%= blog._id %>">Read More</a>
</div>
<% }); %>

<% include ./partials/footer %>