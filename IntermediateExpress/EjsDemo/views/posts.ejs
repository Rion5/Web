<link rel="stylesheet" href="app.css">
<h1>The Posts Page</h1>
<p>for loop</p>
<% for(var i = 0; i <posts.length; i ++) {%>
    <li> 
        <%= posts[i].title %> - <%= posts[i].author %> 
    </li>
<% } %>
<br>
<p>foreach loop</p>
<% posts.forEach(function(post){ %>
    <li>
        <%= post.title %> - <%= post.author %>
    </li>
<% }) %>